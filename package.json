{
  "name": "react-app-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p prisma:generate-watch graphql:codegen-watch webapp:dev backend:dev",
    "build": "npm-run-all -s webapp:build",
    "lint": "eslint webapp/src backend/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "webapp:dev": "vite",
    "webapp:build": "tsc && vite build",
    "backend:dev": "nodemon --watch backend --ext ts --exec ts-node backend/index.ts",
    "prisma:studio": "npx prisma studio",
    "prisma:generate-watch": "npx prisma generate --watch",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "graphql:codegen-watch": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/server": "^4.7.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@mui/x-data-grid": "^6.8.0",
    "@prisma/client": "^4.15.0",
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "random-seed": "^0.3.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
